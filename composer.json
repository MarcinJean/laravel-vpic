{
    "name": "marcinjean/laravel-vpic",
    "description": "Laravel package for VIN decoding via NHTSA vPic API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Jean",
            "email": "Marcin@MarcinJean.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MarcinJean\\LaravelVPic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["MarcinJean\\LaravelVPic\\VPicServiceProvider"],
            "aliases": {"VPic": "MarcinJean\\LaravelVPic\\Facades\\VPic"}
        }
    }
}
